{
  "compilerOptions": {
    "target": "ES6", 
    "module": "commonjs",
    "rootDir": "./src",
    "moduleResolution": "node10",
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "exclude": ["node_modules"]
}
